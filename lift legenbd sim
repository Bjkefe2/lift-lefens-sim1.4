local gb = false
AutoRebirth.Name = "AutoRebirth"
AutoRebirth.Parent = TextLabel_14
AutoRebirth.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
AutoRebirth.BorderColor3 = Color3.fromRGB(60, 60, 60)
AutoRebirth.Position = UDim2.new(1, 0, 0, 0)
AutoRebirth.Size = UDim2.new(0, 35, 0, 20)
AutoRebirth.Font = Enum.Font.SciFi
AutoRebirth.Text = ""
AutoRebirth.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoRebirth.TextSize = 14.000
AutoRebirth.TextSize = 14.000
AutoRebirth.MouseButton1Down:Connect(function()
	if gb == false then
		gb = true
		AutoRebirth.Text = tik
		AutoRebirth.BackgroundColor3 = Color3.fromRGB(0,255,0)
		AutoRebirth.TextColor3 = Color3.fromRGB(0,0,0)
		while gb do
			wait()
			game:GetService"ReplicatedStorage".Network.Port1:FireServer("Purchase","Rank")

		end

	else
		gb = false
		AutoRebirth.Text = ""
		AutoRebirth.BackgroundColor3 = Color3.fromRGB(30,30,30)
	end
end)
